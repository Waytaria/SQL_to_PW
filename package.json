{
  "name": "SQL-to-Edu-Document",
  "displayName": "Educational Work Document from SQL",
  "description": "This project automatically generates a complete SQL Practical Work document as an interactive HTML page from a SQL file. Questions, SQL queries, optional solutions, and query results are extracted and formatted.",
  "version": "1.0.1",
  "publisher": "Waytaria",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Waytaria/generate-sql-tp"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "activationEvents": [
    "onCommand:extension.SQL-to-Doc"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.SQL-to-Doc",
        "title": "Create the HTML document from SQL file analysis"
      }
    ]
  },
  "dependencies": {},
  "scripts": {
    "prepublish": "vsce package",
    "test": "echo \"No test specified\" && exit 0"
  },
  "keywords": [
    "SQL",
    "TP",
    "Education",
    "VSCode",
    "Python"
  ],
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "homepage": "https://github.com/Waytaria/generate-sql-tp",
  "bugs": {
    "url": "https://github.com/Waytaria/generate-sql-tp/issues",
    "email": "contact@thomas-ferragu.me"
  },
  "pythonDependencies": [
    "sqlite3",
    "json",
    "os",
    "argparse"
  ]
}